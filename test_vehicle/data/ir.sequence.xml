<odoo>
    <record id="seq_drivers" model="ir.sequence">
        <field name="name">Vehicle Drivers</field>
        <field name="code">driver.information</field>
        <field name="prefix">Driver</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_vehicle" model="ir.sequence">
        <field name="name">Vehicle Type</field>
        <field name="code">vehicle.type</field>
        <field name="prefix">Veh</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_deposit" model="ir.sequence">
        <field name="name">Ticket Deposit</field>
        <field name="code">ticket.deposit</field>
        <field name="prefix">DEP</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_transaction" model="ir.sequence">
        <field name="name">Daily Transaction</field>
        <field name="code">daily.transaction</field>
        <field name="prefix">TRAN</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_balance" model="ir.sequence">
        <field name="name">Check Balance</field>
        <field name="code">check.balance</field>
        <field name="prefix">Bal</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>